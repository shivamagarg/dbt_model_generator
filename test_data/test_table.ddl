CREATE TABLE TEST_SCHEMA.TEST_TABLE (
    ID INT NOT NULL,
    NAME VARCHAR(100),
    VALUE DECIMAL(10,2),
    STATUS VARCHAR(10),
    DATA_SRC VARCHAR(200),
    CREATE_DT TIMESTAMP,
    CREATE_BY VARCHAR(200),
    CREATE_PGM VARCHAR(200),
    UPDATE_DT TIMESTAMP,
    UPDATE_BY VARCHAR(200),
    UPDATE_PGM VARCHAR(200),
    CONSTRAINT PK_TEST_TABLE PRIMARY KEY (ID),
    CONSTRAINT UK_TEST_TABLE UNIQUE (NAME, STATUS)
); 